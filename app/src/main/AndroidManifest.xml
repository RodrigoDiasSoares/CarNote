<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="br.com.CarNote">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PossanteÁlcoolOuGasolina">
        <activity
            android:name=".activity.activitys.GastoComOCarroActivity"
            android:label="@string/title_activity_gasto_com_o_carro"
            android:theme="@style/Theme.PossanteÁlcoolOuGasolina.NoActionBar"></activity>
        <activity android:name=".activity.activitys.EditGastosComOCarroActivity" />
        <activity android:name=".activity.activitys.MainActivity" />
        <activity android:name=".activity.activitys.AlcoolOuGasolinaResoltado" />
        <activity android:name=".activity.activitys.ConsumoPorLitroResoltado" />
        <activity android:name=".activity.activitys.AlcoolOuGasolina" />
        <activity android:name=".activity.activitys.ConsumoPorLitro" />
        <activity android:name=".activity.activitys.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-9067087122803339~9743228112" />
    </application>

</manifest>